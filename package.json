	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"start": "ENVFILE=.dev.env react-native start",
		"test": "jest",
		"lint": "eslint .",
		"run-dev-ios": "ENVFILE=.dev.env react-native run-ios",
		"build-android": "cd android && ENVFILE=.dev.env ./gradlew assembleRelease",
		"ios-with-env": "ENVFILE=.dev.env react-native run-ios --simulator=\"my_ipad\"",
		"id": "ENVFILE=.dev.env react-native run-ios --simulator=\"iPad Pro (11-inch)\"",
		"ad": "ENVFILE=.dev.env react-native run-android",
		"android-with-env-dev": "ENVFILE=.dev.env react-native run-android",
		"android-with-env-dev-rl": "ENVFILE=.dev.env react-native run-android --variant=release",
		"android-with-env-rl": "ENVFILE=.pro.env react-native run-android --variant=release",
		"android-with-env-pro": "ENVFILE=.pro.env react-native run-android",
		"build-android-pro": "cd android && ENVFILE=.pro.env ./gradlew assembleRelease"
	},
